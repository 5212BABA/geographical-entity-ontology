        <AbbreviatedIRI>obo:GEO_000000392</AbbreviatedIRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">Examples: private bond-rating agencies such as Moody&apos;s Investors Service, International Chamber of Commerce, Worldwide Responsible Apparel Manufacturing Principles, Forest Stewardship</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
